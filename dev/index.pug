doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport", content="width=device-width, initial-scale=1.0")
    meta(http-equiv="X-UA-Compatible", content="ie=edge")
    title Document
  body
    - const names = ['dorian', 'Laura', 'maruta', 'carLos']

    each name in names
      p= name

    each name in names
      p= `${name.charAt(0).toUpperCase()}`

    each name in names
      p= `${name.substring(1)}`